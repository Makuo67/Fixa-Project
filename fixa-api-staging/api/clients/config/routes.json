{
  "routes": [
    {
      "method": "GET",
      "path": "/clients",
      "handler": "clients.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/custom/clients",
      "handler": "clients.getAllClient",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/clients/count",
      "handler": "clients.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/clients/:id",
      "handler": "clients.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/clients",
      "handler": "clients.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/clients/app",
      "handler": "clients.saveClient",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/clients/:id",
      "handler": "clients.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/custom/clients/:client_id",
      "handler": "clients.updateClient",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/clients/:id",
      "handler": "clients.delete",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/custom/clients/:client_id",
      "handler": "clients.deleteClient",
      "config": {
        "policies": []
      }
    }
  ]
}
