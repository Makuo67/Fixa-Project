{
  "routes": [
    {
      "method": "GET",
      "path": "/rates",
      "handler": "rates.find",
      "config": {
        "policies": ["global::project_policies"]
      }
    },
    {
      "method": "GET",
      "path": "/rates/count",
      "handler": "rates.count",
      "config": {
        "policies": ["global::project_policies"]
      }
    },
    {
      "method": "GET",
      "path": "/rates/:id",
      "handler": "rates.findOne",
      "config": {
        "policies": ["global::project_policies"]
      }
    },
    {
      "method": "POST",
      "path": "/rates",
      "handler": "rates.create",
      "config": {
        "policies": ["global::project_policies"]
      }
    },
    {
      "method": "POST",
      "path": "/rates/many",
      "handler": "rates.createManyRates",
      "config": {
        "policies": ["global::project_policies"]
      }
    },
    {
      "method": "PUT",
      "path": "/rates/:id",
      "handler": "rates.update",
      "config": {
        "policies": ["global::project_policies"]
      }
    },
    {
      "method": "DELETE",
      "path": "/rates/:id",
      "handler": "rates.delete",
      "config": {
        "policies": ["global::project_policies"]
      }
    }
  ]
}
